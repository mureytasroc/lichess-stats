{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "react",
        "prettier"

    ],
    "extends": [
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "react/jsx-filename-extension": 0,
        "no-unused-vars": [
            "error",
            {
              "args": "none"
            }
          ]
    }
}
