{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "react",
        "prettier"

    ],
    "extends": [
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "react/jsx-props-no-spreading" : 0
    }
}
